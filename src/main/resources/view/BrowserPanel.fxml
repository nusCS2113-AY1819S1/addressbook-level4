<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<StackPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <WebView fx:id="browser" opacity="0.0" prefHeight="200.0" prefWidth="200.0" />
      <VBox fx:id="display" prefHeight="500.0" prefWidth="600.0" spacing="5.0">
         <children>
            <VBox alignment="CENTER_LEFT" prefHeight="259.0" prefWidth="600.0" spacing="5.0" style="-fx-background-color: white; -fx-background-radius: 5 5 5 5; -fx-border-radius: 5 5 5 5;">
               <children>
                  <Label fx:id="nameLabel" style="-fx-text-fill: black;">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="59.0" pickOnBounds="true" preserveRatio="true" translateX="-2.0">
                           <image>
                              <Image url="@../images/avatar.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Label>
                  <HBox prefHeight="21.0" prefWidth="395.0" spacing="5.0">
                     <children>
                        <ImageView fitHeight="20.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true" translateX="-2.0">
                           <image>
                              <Image url="@../images/tag.png" />
                           </image>
                        </ImageView>
                        <FlowPane fx:id="tagPanel" hgap="5.0" prefHeight="33.0" prefWidth="375.0" vgap="5.0" />
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="5.0" left="20.0" top="5.0" />
               </padding>
            </VBox>
            <VBox alignment="CENTER_LEFT" prefHeight="267.0" prefWidth="400.0" spacing="5.0" style="-fx-background-color: white; -fx-background-radius: 5 5 5 5; -fx-border-radius: 5 5 5 5;" VBox.vgrow="ALWAYS">
               <children>
                  <Region prefHeight="200.0" prefWidth="200.0" />
                  <Label fx:id="addressLabel" style="-fx-text-fill: black;">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="59.0" pickOnBounds="true" preserveRatio="true" translateX="-2.0">
                           <image>
                              <Image url="@../images/house.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Label>
                  <Region prefHeight="200.0" prefWidth="200.0" />
                  <Label fx:id="phoneLabel" style="-fx-text-fill: black;">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="59.0" pickOnBounds="true" preserveRatio="true" translateX="-2.0">
                           <image>
                              <Image url="@../images/phone-call.png" />
                           </image>
                           <viewport>
                              <Rectangle2D />
                           </viewport>
                        </ImageView>
                     </graphic>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Label>
                  <Region prefHeight="200.0" prefWidth="200.0" />
                  <Label fx:id="emailLabel" centerShape="false" style="-fx-text-fill: black;">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="59.0" pickOnBounds="true" preserveRatio="true" translateX="-2.0">
                           <image>
                              <Image url="@../images/envelope.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Label>
                  <Region prefHeight="200.0" prefWidth="200.0" />
                  <Label fx:id="kpiLabel" style="-fx-text-fill: black;" text="KPI:">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Label>
                  <Region prefHeight="200.0" prefWidth="200.0" />
                  <Label fx:id="noteLabel" style="-fx-text-fill: black;">
                     <graphic>
                        <ImageView fitHeight="20.0" fitWidth="59.0" pickOnBounds="true" preserveRatio="true" translateX="-2.0">
                           <image>
                              <Image url="@../images/edit.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Label>
                  <Region prefHeight="200.0" prefWidth="200.0" />
               </children>
               <padding>
                  <Insets left="20.0" top="5.0" />
               </padding>
            </VBox>
         </children>
      </VBox>
      <VBox fx:id="welcomePane" alignment="CENTER" style="-fx-background-color: #eeeff1; -fx-background-radius: 5 5 5 5; -fx-border-radius: 5 5 5 5;">
         <children>
            <Label alignment="CENTER" text="Welcome to CorpProC" />
         </children>
      </VBox>
   </children>
</StackPane>
